{
    "manifest_version": 2,
    "name": "Sustainable Alternative Finder",
    "version": "1",
    "content_scripts": [
        {
            "matches": [
                "https://shop.bigbazaar.com/*"
            ],
            "js": ["bigbazaar/script.js"],
            "css": ["bigbazaar/styles.css"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "data/links.txt"
    ]
}